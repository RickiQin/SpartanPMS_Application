/************	OVERWRITE FOR SMALLER SEARCH BOX ********************/


.rightSideWrapper .searchForm {
	width: 310px;
	height: 290px;
	display: block;
	float: none;
}

.rightSideWrapper .searchForm input[type=text], .rightSideWrapper .searchForm select {
	font-size: 14px;
	height: 25px;
}

.rightSideWrapper .searchForm label {
	font-size: 18px;
}

.rightSideWrapper .searchForm .mainsearch input {
	width: 250px;
	margin-right: 10px;
}

.rightSideWrapper .searchForm .subsearch input[type=text], .rightSideWrapper .searchForm .subsearch select {
	width: 50px;
}

.rightSideWrapper .searchForm .submitsearch .btn {
	width: 198px;
	height: 30px;
}

.rightSideWrapper .searchForm .subsearch {
	margin: 10px 25px;
}

.rightSideWrapper .mapContainer {
	width: 310px;
	height: 310px;
	display: block;
	position: relative;
}

.rightSideWrapper #map-canvas {
	width: 300px;
	margin: 5px;
	height: 300px;
	border: 5px solid #13398E;
	display: block;
	position: relative;
}

.pagination {
	width: 100%;
	display: block;
	text-align: center;
	overflow: hidden;
	background: #CCC;
	color: #555;
}

.pagination ul {
	position: relative;
	float: none;
	overflow: hidden;
	margin: 5px auto;
	display: inline-block;
}

.pagination ul li {
	display: inline-block;
}

#pagination-clean li {
	border: 0;
	margin: 0;
	padding: 0;
	font-size: 13px;
	list-style: none;
	float: left;
}

#pagination-clean li, #pagination-clean a {
	margin-right: 2px;
}

#pagination-clean .previous-off, #pagination-clean .next-off {
	color: #888888 display :block;
	float: left;
	font-weight: bold;
	padding: 3px 4px;
}

#pagination-clean .next a, #pagination-clean .previous a {
	font-weight: bold;
}

#pagination-clean .active {
	color: #00000 font-weight :bold;
	display: block;
	float: left;
	padding: 3px 6px;
}

#pagination-clean a:link, #pagination-clean a:visited {
	color: #0033CC; 
	display: block;
	float: left;
	padding: 3px 6px;
	text-decoration: none;
}

#pagination-clean a:hover {
	text-decoration: none;
}

.noResultsFound {
	text-align: center;
	padding: 15px;
}
