
.searchResultsWrapper, .rightSideWrapper {
	float: left;
	display: inline-block;
	overflow: hidden;
	margin: 0;
	padding: 0;
}

.searchResultsWrapper {
	width: 622px;
	min-height: 600px;
}

.rightSideWrapper {
	width: 320px;
	margin: 0 5px;
	padding-top: 32px;
}

