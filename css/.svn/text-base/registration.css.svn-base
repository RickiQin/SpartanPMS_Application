/*For LoginPage & RegiPage*/
.searchForm h5{
	color: #0F3164;
    font-family: Gill Sans MT;
    font-size: 22.98px;
    padding: 50px 0 0 100px;
}

.typeuser{
	margin-top:20px;
	margin-left:20px;
	font-family: Gill Sans MT;
	font-size:22px;
	color:#000000;
}

.typeuser input{
	font-family: Gill Sans MT;
	font-size:16px;
	color:#000000;
	margin-left:7px;
	padding-left:7px;
	border: 2px solid #434343;
}

.typepw{
	margin-left:-17px;
	margin-top:20px;
	font-family: Gill Sans MT;
	font-size:22px;
	color:#000000;
}

.typepw input{
	font-family: Gill Sans MT;
	font-size:16px;
	color:#000000;
	margin-left:7px;
	padding-left:7px;
	border: 2px solid #434343;
}

.submitlogin {
	margin: 10px 0 0 100px;
	width: 178px;
	height: 70px;
	cursor:pointer;
}

.searchForm h6{
	color: #0F3164;
    font-family: Gill Sans MT;
    font-size: 22.98px;
    padding: 5px 0 0 100px;
}

.typename{
	margin-top:10px;
	font-family: Gill Sans MT;
	font-size:20px;
	color:#000000;
	margin-left: 10px;
}

.typename input, .typename select{
	font-family: Gill Sans MT;
	font-size:16px;
	color:#000000;
	margin-left:7px;
	padding: 2px 7px;
	border: 2px solid #434343;
}

.typeemail{
	margin-top:20px;
	font-family: Gill Sans MT;
	font-size:20px;
	color:#000000;
}

